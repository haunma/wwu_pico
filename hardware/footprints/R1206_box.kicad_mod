(module R1206_box (layer F.Cu) 
  (at 0 0)
  (attr smd)
  (descr "Resistor SMD 1206, reflow soldering, Vishay, box on silkscreen")
  (fp_text reference R (at 0 -1.85) (layer F.SilkS)
    (effects (font (size 1 1) (thickness 0.15)))
  )
  (fp_line (start 2.15 1.1) (end -2.15 1.1) (layer F.SilkS) (width 0.12))
  (fp_line (start 2.15 1.1) (end 2.15 -1.11) (layer F.SilkS) (width 0.12))
  (fp_line (start -2.15 -1.11) (end -2.15 1.1) (layer F.SilkS) (width 0.12))
  (fp_line (start -2.15 -1.11) (end 2.15 -1.11) (layer F.SilkS) (width 0.12))
  (pad 1 smd rect (at -1.45 0) (size 0.9 1.7) (layers F.Cu F.Paste F.Mask))
  (pad 2 smd rect (at 1.45 0) (size 0.9 1.7) (layers F.Cu F.Paste F.Mask))
  (model ${KISYS3DMOD}/Resistors_SMD.3dshapes/R_1206.wrl
    (at (xyz 0 0 0))
    (scale (xyz 1 1 1))
    (rotate (xyz 0 0 0))
  )
)
